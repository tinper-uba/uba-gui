{
  "presets": [
    "env",
    "react",
    "stage-2"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": true,
        "regenerator": true
      }
    ],
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }]
  ]
}